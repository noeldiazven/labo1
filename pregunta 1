#include<iostream>
#include <ctime>
#include <stdlib.h>
using namespace std;
https://github.com/senior-gato/Computacion-Paralela-y-Distribuida/tree/master/Laboratorio1
void example_1(){
	int max = 100;
	double A[max][max];
	double X[max]; 
	double Y[max];
	for (int i = 0;i<max;i=i+1){
		for (int j=0;j<max;j=j+1){
			Y[i] += A[i][j]*X[j];
		}
	}
}

void example_2(){
	unsigned t0, t1;
	int max = 100;
	double A[max][max];
	double X[max]; 
	double Y[max];
	t0=clock();
	for (int j = 0; j<max;j= j+1){
		for(int i = 0;i<max;i =i+1){
			Y[i] += A[i][j]*X[j];	
		}
	}
	t1 = clock();
	double time = (double(t1-t0)/CLOCKS_PER_SEC);
	cout << "tiempo ejemplo 2: " << time << endl;
}

int main(){
	unsigned t0, t1;
	t0 = clock();
 	example_1();
	t1 = clock();
	double time = (double(t1-t0)/CLOCKS_PER_SEC);
	cout << "Execution Time: " << time << endl;
	example_2();
	return 0;
}
